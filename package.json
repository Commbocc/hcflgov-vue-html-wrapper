{
  "name": "@hcflgov/vue-html-wrapper",
  "version": "3.0.1",
  "private": false,
  "description": "A Vue.js plugin",
  "author": "Kevin McCarthy <mccarthyk@hcflgov.net>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name HcHtmlWrapper --formats umd-min",
    "lint": "vue-cli-service lint",
    "build-all": "npm run build && npm run build-demo && npm run styleguide:build",
    "build-demo": "vue-cli-service build --dest demo",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "main": "dist/HcHtmlWrapper.umd.min.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "leaflet": "^1.6.0",
    "popper.js": "^1.14.3",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lodash.debounce": "^4.0.8",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-styleguidist": "^3.8.2",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.5.21",
    "vuetify": "^1.5.5",
    "vuex": "^3.1.2"
  },
  "homepage": "https://github.com/Commbocc/hcflgov-vue-html-wrapper",
  "license": "MIT",
  "style": "dist/HcHtmlWrapper.css"
}
